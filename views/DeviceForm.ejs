<h1 align="center">Device ADD</h1>
<div class="container">
    <div class="row mt-5">
        <div class="col-md-12 d-flex justify-content-center">
            <form action="/Device/add" method="post" class="w-50">
                <div class="form-group">
                    <label for="HostIP">HostIP</label>
                    <input type="text" name="HostIP" placeholder="HostIP" class="form-control">
                </div>
                <div class="form-group">
                    <label for="DeviceID">DeviceName</label>
                    <input type="text" name="DeviceName" placeholder="DeviceName" class="form-control">
                </div>
                <br>
                <button type="submit" class="btn btn-primary">บันทึก</button>
                <a href="/Device/list" class="btn btn-primary">ย้อนกลับ</a>
            </form>
        </div>
    </div>
</div>
<%- include("_footer"); -%>
